<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record forcecreate="True" id="gt_import_shopify_order_schedular" model="ir.cron">
        <field name="name">Import Shopify Orders</field>
        <field name="model_id" ref="model_gt_shopify_instance"/>
        <field name="state">code</field>
        <field name="code">model.gt_shopify_import_order_schedular(True)</field>
        <field eval="False" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
