			<field name="model_id" ref="melisync.model_melisync_shipping_methods_options" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
		<!-- Add default permissions to base.user_root (for crons) -->
		<record model="res.groups" id="melisync.res_groups_base_user_root_permissions_administrator">
			<field name="name">MercadoLibre Administrator</field>
			<field name="users" eval="[(4, ref('base.user_root'))]"/>
			<field name="implied_ids" eval="[(4, ref('melisync.res_groups_administrator'))]"></field>
		</record>
		<record model="res.groups" id="melisync.res_groups_base_user_root_permissions_sales_administrator">
			<field name="name">MercadoLibre Sales administrator</field>
			<field name="users" eval="[(4, ref('base.user_root'))]"/>
