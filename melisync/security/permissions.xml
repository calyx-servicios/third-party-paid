		</record>
		<record model="res.groups" id="melisync.res_groups_base_user_root_permissions_products_moderator">
			<field name="name">MercadoLibre Products Moderator</field>
			<field name="users" eval="[(4, ref('base.user_root'))]"/>
			<field name="implied_ids" eval="[(4, ref('melisync.res_groups_products_moderator'))]"></field>
		</record>
    </data>
</odoo>