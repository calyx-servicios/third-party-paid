<odoo>
    <data noupdate="1">
        <!-- Module category -->
        <record model="ir.module.category" id="melisync.res_groups_administrator_module_category">
            <field name="name">MercadoLibre Administration</field>
            <field name="description">The MercadoLibre Administration Application.</field>
            <field name="sequence">10</field>
        </record>
        <!-- Groups -->
        <record model="res.groups" id="melisync.res_groups_products_moderator">
            <field name="name">MercadoLibre Products Moderation</field>
            <field name="comment">MercadoLibre Products Moderation Permission Group.</field>
            <field name="category_id" ref="melisync.res_groups_administrator_module_category"/>
        </record>
        <record model="res.groups" id="melisync.res_groups_sales_administrator">
            <field name="name">MercadoLibre Sales Administration</field>
            <field name="comment">MercadoLibre Sales Administration Permission Group.</field>
            <field name="category_id" ref="melisync.res_groups_administrator_module_category"/>
    		<field name="implied_ids" eval="[(6, False, [ref('melisync.res_groups_products_moderator')])]" />
        </record>
        <record model="res.groups" id="melisync.res_groups_administrator">
            <field name="name">MercadoLibre Settings Administration</field>
            <field name="comment">MercadoLibre Settings Administration Permission Group.</field>
            <field name="category_id" ref="melisync.res_groups_administrator_module_category"/>
    		<field name="implied_ids" eval="[(6, False, [ref('melisync.res_groups_sales_administrator')])]" />
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        </record>
        <!-- Access rights -->
        <record model="ir.model.access" id="melismodel_access_model_melisync_categories">
			<field name="name">MercadoLibre Settings Administration Access Rights (Categories)</field>
			<field name="model_id" ref="melisync.model_melisync_categories" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
        <record model="ir.model.access" id="melisync.ir_model_access_model_melisync_site_ids">
			<field name="name">MercadoLibre Settings Administration Access Rights (Site IDS)</field>
			<field name="model_id" ref="melisync.model_melisync_site_ids" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
        <record model="ir.model.access" id="melisync.ir_model_access_model_melisync_listing_types">
			<field name="name">MercadoLibre Settings Administration Access Rights (Listing Types)</field>
			<field name="model_id" ref="melisync.model_melisync_listing_types" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
        <record model="ir.model.access" id="melisync.ir_model_access_model_melisync_product_attribute_tags">
			<field name="name">MercadoLibre Settings Administration Access Rights (Product Attribute Tags)</field>
			<field name="model_id" ref="melisync.model_melisync_product_attribute_tags" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
        <record model="ir.model.access" id="melisync.ir_model_access_model_melisync_product_attribute_tags">
			<field name="name">MercadoLibre Settings Administration Access Rights (Product Attribute Tags)</field>
			<field name="model_id" ref="melisync.model_melisync_product_attribute_tags" />
			<field name="group_id" ref="melisync.res_groups_administrator" />
			<field name="perm_read" eval="True" />
			<field name="perm_create" eval="True" />
			<field name="perm_write" eval="True" />
			<field name="perm_unlink" eval="True" />
		</record>
        <record model="ir.model.access" id="melisync.ir_model_access_model_base_res_currency">
			<field name="name">MercadoLibre Settings Administration Access Rights (res.currency)</field>
